#include <stdio.h>

int main() {
	float celsius, fahrenheit;
	
	printf("Tabela de equivalencia de temperatura:\n");
	printf("Celsius\tFahrenheit\n");
	
	for (celsius = 0; celsius <= 100; celsius++) {
		fahrenheit = (9.0 / 5.0) * celsius + 32;
		printf("%.1f\t%.1f\n", celsius, fahrenheit);
	}
	
	return 0;
	
}
