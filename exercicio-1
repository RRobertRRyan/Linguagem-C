# Linguagem-C
#include <stdio.h>

int main() {
	int numero, i;
	
	printf("Digite um numero entre 1 e 10: ");
	scanf("%d", &numero);
	
	if (numero < 1 || numero > 10) {
		printf("Numero invalido. Por favor, insira um numero entre 1 e 10.\n");
		return 1;
	}
	
	for (i = 0; i < numero; i++) {
		printf("%d\n", numero);
	}
	
	return 0;
	
}
